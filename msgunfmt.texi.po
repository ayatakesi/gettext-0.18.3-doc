msgstr "--csharp-resources@r{, @code{msgunfmt} option}"

#. type: cindex
#: msgunfmt.texi.orig:29
#, no-wrap
msgid "C# resources mode, and @code{msgunfmt} program"
msgstr "C# resources mode, and @code{msgunfmt} program"

#. type: table
#: msgunfmt.texi.orig:31
msgid "C# resources mode: input is a .NET @file{.resources} file."
msgstr "C# resourcesモード: .NETの@file{.resources}ファイルが入力となります。"

#. type: item
#: msgunfmt.texi.orig:32
#, no-wrap
msgid "--tcl"
msgstr "--tcl"

#. type: opindex
