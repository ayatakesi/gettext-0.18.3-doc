#. type: pindex
#: msgunfmt.texi.orig:1
#, no-wrap
msgid "msgunfmt"
msgstr "msgunfmt"

#. type: cindex
#: msgunfmt.texi.orig:2
#, no-wrap
msgid "@code{msgunfmt} program, usage"
msgstr "@code{msgunfmt} program, usage"

#. type: example
#: msgunfmt.texi.orig:5
#, no-wrap
msgid "msgunfmt [@var{option}] [@var{file}]...\n"
msgstr "msgunfmt [@var{option}] [@var{file}]...\n"

#. type: cindex
#: msgunfmt.texi.orig:7
#, no-wrap
msgid "convert binary message catalog into PO file"
msgstr "convert binary message catalog into PO file"

#. type: Plain text
#: msgunfmt.texi.orig:10
msgid ""
"The @code{msgunfmt} program converts a binary message catalog to a Uniforum "
"style .po file."
msgstr "@code{msgunfmt}は、バイナリーのメッセージカタログを、Uniforum形式の.poファイルに変換するプログラムです。"

