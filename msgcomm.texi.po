#: msgcomm.texi.orig:199
#, no-wrap
msgid "-V@r{, @code{msgcomm} option}"
msgstr "-V@r{, @code{msgcomm} オプション}"

#. type: opindex
#: msgcomm.texi.orig:200
#, no-wrap
msgid "--version@r{, @code{msgcomm} option}"
msgstr "--version@r{, @code{msgcomm} オプション}"

#. type: table
#: msgcomm.texi.orig:202
msgid "Output version information and exit."
msgstr "バージョン情報を表示して終了します。"
