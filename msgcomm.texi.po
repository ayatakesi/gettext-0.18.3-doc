#: msgcomm.texi.orig:192
#, no-wrap
msgid "--help"
msgstr "--help"

#. type: opindex
#: msgcomm.texi.orig:193
#, no-wrap
msgid "-h@r{, @code{msgcomm} option}"
msgstr "-h@r{, @code{msgcomm} オプション}"

#. type: opindex
#: msgcomm.texi.orig:194
#, no-wrap
msgid "--help@r{, @code{msgcomm} option}"
msgstr "--help@r{, @code{msgcomm} オプション}"

#. type: table
#: msgcomm.texi.orig:196
