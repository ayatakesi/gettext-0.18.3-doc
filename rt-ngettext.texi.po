#. type: pindex
#: rt-ngettext.texi.orig:1
#, no-wrap
msgid "ngettext"
msgstr ""

#. type: cindex
#: rt-ngettext.texi.orig:2
#, no-wrap
msgid "@code{ngettext} program, usage"
msgstr ""

#. type: example
#: rt-ngettext.texi.orig:5
#, no-wrap
msgid ""
"ngettext [@var{option}] [@var{textdomain}] @var{msgid} @var{msgid-plural} "
"@var{count}\n"
msgstr ""

#. type: cindex
#: rt-ngettext.texi.orig:7
#, no-wrap
msgid "lookup plural message translation"
msgstr ""

#. type: Plain text
#: rt-ngettext.texi.orig:10
msgid ""
"The @code{ngettext} program displays the native language translation of a "
"textual message whose grammatical form depends on a number."
msgstr ""

#. type: item
#: rt-ngettext.texi.orig:14
#, no-wrap
msgid "-d @var{textdomain}"
msgstr ""

#. type: itemx
#: rt-ngettext.texi.orig:15
#, no-wrap
msgid "--domain=@var{textdomain}"
msgstr ""

#. type: opindex
#: rt-ngettext.texi.orig:16
#, no-wrap
msgid "-d@r{, @code{ngettext} option}"
msgstr ""

#. type: opindex
#: rt-ngettext.texi.orig:17
#, no-wrap
msgid "--domain@r{, @code{ngettext} option}"
msgstr ""

#. type: table
#: rt-ngettext.texi.orig:20
msgid ""
"Retrieve translated messages from @var{textdomain}.  Usually a "
"@var{textdomain} corresponds to a package, a program, or a module of a "
"program."
msgstr ""

#. type: item
#: rt-ngettext.texi.orig:21
#, no-wrap
msgid "-e"
msgstr ""

#. type: opindex
#: rt-ngettext.texi.orig:22
#, no-wrap
msgid "-e@r{, @code{ngettext} option}"
msgstr ""

#. type: table
#: rt-ngettext.texi.orig:28
msgid ""
"Enable expansion of some escape sequences.  This option is for compatibility "
"with the @samp{gettext} program.  The escape sequences @samp{\\a}, "
"@samp{\\b}, @samp{\\c}, @samp{\\f}, @samp{\\n}, @samp{\\r}, @samp{\\t}, "
"@samp{\\v}, @samp{\\\\}, and @samp{\\} followed by one to three octal "
"digits, are interpreted like the System V @samp{echo} program did."
msgstr ""

#. type: item
#: rt-ngettext.texi.orig:29
#, no-wrap
msgid "-E"
msgstr ""

#. type: opindex
#: rt-ngettext.texi.orig:30
#, no-wrap
msgid "-E@r{, @code{ngettext} option}"
msgstr ""

#. type: table
#: rt-ngettext.texi.orig:33
msgid ""
"This option is only for compatibility with the @samp{gettext} program.  It "
"has no effect."
msgstr ""

#. type: item
#: rt-ngettext.texi.orig:34
#, no-wrap
msgid "-h"
msgstr ""

#. type: itemx
#: rt-ngettext.texi.orig:35
#, no-wrap
msgid "--help"
msgstr ""

#. type: opindex
#: rt-ngettext.texi.orig:36
#, no-wrap
msgid "-h@r{, @code{ngettext} option}"
msgstr ""

#. type: opindex
#: rt-ngettext.texi.orig:37
#, no-wrap
msgid "--help@r{, @code{ngettext} option}"
msgstr ""

#. type: table
#: rt-ngettext.texi.orig:39
msgid "Display this help and exit."
msgstr ""

#. type: item
#: rt-ngettext.texi.orig:40
#, no-wrap
msgid "-V"
msgstr ""

#. type: itemx
#: rt-ngettext.texi.orig:41
#, no-wrap
msgid "--version"
msgstr ""

#. type: opindex
#: rt-ngettext.texi.orig:42
#, no-wrap
