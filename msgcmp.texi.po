#: msgcmp.texi.orig:66
#, no-wrap
msgid "-P"
msgstr "-P"

#. type: itemx
#: msgcmp.texi.orig:67
#, no-wrap
msgid "--properties-input"
msgstr "--properties-input"

#. type: opindex
#: msgcmp.texi.orig:68
#, no-wrap
